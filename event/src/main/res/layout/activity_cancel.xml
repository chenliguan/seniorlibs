<?xml version="1.0" encoding="utf-8"?>
<com.seniorlibs.event.cancel.MyScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".dispatch.DispatchActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <com.seniorlibs.event.cancel.CancelView
            android:id="@+id/green1"
            android:layout_width="300dp"
            android:layout_height="200dp"
            android:background="@color/colorPrimary"
            android:textAllCaps="false"/>

        <com.seniorlibs.event.cancel.CancelView
            android:id="@+id/green2"
            android:layout_width="300dp"
            android:layout_height="200dp"
            android:background="@color/colorPrimary"
            android:layout_marginTop="100dp"
            android:textAllCaps="false"/>

        <com.seniorlibs.event.cancel.CancelView
            android:id="@+id/green3"
            android:layout_width="300dp"
            android:layout_height="200dp"
            android:background="@color/colorPrimary"
            android:layout_marginTop="100dp"
            android:textAllCaps="false"/>
    </LinearLayout>
</com.seniorlibs.event.cancel.MyScrollView>

