<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <Button
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:onClick="synchronizedTest"
            android:text="synchronizedTest"
            android:textSize="16sp" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:onClick="atomicIntegerTest"
            android:text="atomicIntegerTest"
            android:textSize="16sp" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:onClick="testWriteArrayListError"
            android:text="testWriteArrayListError"
            android:textSize="16sp" />
        <Button
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:onClick="testAcquire"
            android:text="testAcquire"
            android:textSize="16sp" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:onClick="testTryAcquire"
            android:text="testTryAcquire"
            android:textSize="16sp" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:onClick="textrejectedExecution"
            android:text="textrejectedExecution"
            android:textSize="16sp" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:onClick="testHashSet"
            android:text="testHashSet"
            android:textSize="16sp" />
    </LinearLayout>
</ScrollView>