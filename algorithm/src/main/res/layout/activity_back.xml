<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/BaseMatchParent"
    android:orientation="vertical"
    tools:context=".main.MainActivity"
    tools:ignore="MissingDefaultResource">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="vertical">

        <Button
            android:id="@+id/btn_bracket_generate"
            style="@style/BtnStyles"
            android:text="22. 括号生成" />

        <Button
            android:id="@+id/btn_permute"
            style="@style/BtnStyles"
            android:text="46. 全排列" />

        <Button
            android:id="@+id/btn_permutation"
            style="@style/BtnStyles"
            android:text="剑指 Offer 38. 字符串的排列" />

        <Button
            android:id="@+id/btn_subsets"
            style="@style/BtnStyles"
            android:text="78. 子集" />

        <Button
            android:id="@+id/btn_combine"
            style="@style/BtnStyles"
            android:text="77. 组合" />

        <Button
            android:id="@+id/btn_combination_sum"
            style="@style/BtnStyles"
            android:text="39. 组合总和" />

        <Button
            android:id="@+id/btn_restore_ip_addresses"
            style="@style/BtnStyles"
            android:text="93. 复原 IP 地址" />

        <Button
            android:id="@+id/btn_letter_combinations"
            style="@style/BtnStyles"
            android:text="17. 电话号码的字母组合" />


        <Button
            android:id="@+id/btn_solve_n_queens"
            style="@style/BtnStyles"
            android:text="51. N 皇后" />
    </LinearLayout>
</ScrollView>